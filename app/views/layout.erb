<html>
  <head>
    <title>Bookmark Manager</title>
  </head>

  <body>

    <% if current_user %>
      Welcome, <%= current_user.username %>
    <% end %>

    <% if @errors %>
      <div id='error'><% @errors.each do |error| %>
        <%= error %> </div> <% end%> 
    <% end %>

    <%= yield %>

  </body>
</html>
